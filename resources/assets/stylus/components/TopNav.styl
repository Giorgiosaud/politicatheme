@media (min-width: 768px)
	.TopNav__Item
		margin: 5px 0;
		border-right: solid #6e6e6e 1px;
		&:first-child{
			border-left: solid #6e6e6e 1px;
		}
		&:last-child{
			margin-right: 3px;
		}
