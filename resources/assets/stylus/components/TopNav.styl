.TopNav
	background-color: #eee
	border-color: #eee
	min-height: 0
	font-size 0.9em
	.navbar-toggle
		padding: 1em 0
		margin: 0 3em
		.icon-bar
			background black
.TopNav__Item
	&.active
		background darken(#eee,50) !important
		&>a
			color #FFF !important
	a
		color #6e6e6e !important
		padding 0px 15px 0 15px !important
	& > a:hover, & > a:focus
		background-color darken(#eee,30) !important
		color lighten(#eee,30) !important
